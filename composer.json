{
  "name": "apivalk/apivalk",
  "archive": {
    "exclude": [
      "/.github",
      "/.gitignore",
      "/phpunit.xml",
      "/vendor"
    ]
  },
  "description": "A Lightweight, Framework-Agnostic REST API Ecosystem for PHP. Built for speed, precision, and type-safe development.",
  "keywords": [
    "openapi",
    "rest",
    "api",
    "php",
    "type-safe",
    "psr-7",
    "psr-15"
  ],
  "homepage": "https://apivalk.com",
  "license": "MIT",
  "config": {
    "platform": {
      "php": "7.2.34"
    }
  },
  "authors": [
    {
      "name": "Dominic Poppe",
      "email": "dominic@dopo.dev"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/container": "^1.1.1 || ^2.0.1",
    "psr/log": "^1.1",
    "firebase/php-jwt": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "phpstan/phpstan": "^1.12"
  },
  "autoload": {
    "psr-4": {
      "apivalk\\apivalk\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}
